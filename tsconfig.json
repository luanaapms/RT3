{
    "compilerOptions": {
      "target": "ES6",
      "module": "ESNext",
      "strict": false,
      "esModuleInterop": true,
      "skipLibCheck": true,
      "forceConsistentCasingInFileNames": true,
      "outDir": "out/",
      "moduleResolution": "node",
      "noImplicitAny": true
    },
    "include": [
      "src/**/*"
    ]
}